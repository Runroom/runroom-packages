{
    "name": "runroom-packages/doctrine-translatable-bundle",
    "description": "Manage doctrine translatable entities with Symfony",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "translatable",
        "symfony",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "homepage": "https://github.com/runroom/RunroomDoctrineTranslatableBundle",
    "require": {
        "php": "^8.3",
        "doctrine/collections": "^2.0",
        "doctrine/dbal": "^3.6 || ^4.0",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.19 || ^3.2",
        "doctrine/persistence": "^3.1 || ^4.0",
        "symfony/dependency-injection": "^6.4 || ^7.1",
        "symfony/framework-bundle": "^6.4 || ^7.1",
        "symfony/http-kernel": "^6.4 || ^7.1",
        "symfony/translation-contracts": "^2.4 || ^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "matthiasnoback/symfony-config-test": "^6.1",
        "matthiasnoback/symfony-dependency-injection-test": "^6.1",
        "phpunit/phpunit": "^12.3",
        "symfony/translation": "^6.4 || ^7.1",
        "zenstruck/foundry": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Runroom\\DoctrineTranslatableBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\DoctrineTranslatableBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.21-dev"
        }
    }
}
