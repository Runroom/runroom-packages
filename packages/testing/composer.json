{
    "name": "runroom-packages/testing",
    "description": "This library provides tools to help testing our bundles and projects",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "testing",
        "doctrine",
        "fixtures",
        "integration",
        "phpunit"
    ],
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "homepage": "https://github.com/runroom/testing",
    "require": {
        "php": "^7.4 || ^8.0",
        "phpunit/phpunit": "^9.5",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-foundation": "^4.4.7 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4.13 || ^5.4 || ^6.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.5.1",
        "doctrine/dbal": "^2.10 <2.13.1 || ^3.1.4",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/orm": "^2.8",
        "knplabs/knp-menu-bundle": "^3.1",
        "sonata-project/admin-bundle": "^3.103 || ^4.9",
        "sonata-project/doctrine-orm-admin-bundle": "^3.34 || ^4.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/security-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/twig-bundle": "^4.4 || ^5.4 || ^6.0",
        "theofidry/alice-data-fixtures": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Runroom\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\Testing\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.17-dev"
        }
    }
}
