{
    "name": "runroom-packages/user-bundle",
    "type": "symfony-bundle",
    "description": "Manage admin users for Sonata",
    "keywords": [
        "user",
        "symfony",
        "doctrine",
        "admin",
        "sonata"
    ],
    "homepage": "https://github.com/runroom/RunroomUserBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.9",
        "doctrine/persistence": "^2.0",
        "psr/container": "^1.1 || ^2.0",
        "symfony/config": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/doctrine-bridge": "^5.3",
        "symfony/form": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/http-foundation": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symfony/options-resolver": "^5.3",
        "symfony/password-hasher": "^5.3",
        "symfony/routing": "^5.3",
        "symfony/security-core": "^5.3",
        "symfony/security-http": "^5.3",
        "symfony/translation": "^5.3",
        "symfony/twig-bridge": "^5.3",
        "symfony/validator": "^5.3",
        "twig/twig": "^2.12 || ^3.0"
    },
    "conflict": {
        "sonata-project/admin-bundle": "<4.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^9.5",
        "runroom-packages/testing": "^0.13@dev",
        "sonata-project/admin-bundle": "^4.0",
        "sonata-project/doctrine-orm-admin-bundle": "^4.0",
        "symfony/mailer": "^5.3",
        "symfony/mime": "^5.3",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/security-bundle": "^5.3",
        "symfony/twig-bundle": "^5.3",
        "symfonycasts/reset-password-bundle": "^1.9",
        "zenstruck/foundry": "^1.13.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.13-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\UserBundle\\Tests\\": "tests/"
        }
    }
}
