{
    "name": "runroom-packages/user-bundle",
    "description": "Manage admin users for Sonata",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "user",
        "symfony",
        "doctrine",
        "admin",
        "sonata"
    ],
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "homepage": "https://github.com/runroom/RunroomUserBundle",
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/orm": "^2.14",
        "doctrine/persistence": "^3.1",
        "psr/container": "^1.1 || ^2.0",
        "sonata-project/admin-bundle": "^4.20",
        "symfony/asset": "^5.4 || ^6.4",
        "symfony/config": "^5.4 || ^6.4",
        "symfony/dependency-injection": "^5.4 || ^6.4",
        "symfony/doctrine-bridge": "^5.4 || ^6.4",
        "symfony/form": "^5.4 || ^6.4",
        "symfony/framework-bundle": "^5.4 || ^6.4",
        "symfony/http-foundation": "^5.4 || ^6.4",
        "symfony/http-kernel": "^5.4 || ^6.4",
        "symfony/options-resolver": "^5.4 || ^6.4",
        "symfony/routing": "^5.4 || ^6.4",
        "symfony/security-bundle": "^5.4 || ^6.4",
        "symfony/security-core": "^5.4 || ^6.4",
        "symfony/security-http": "^5.4 || ^6.4",
        "symfony/translation": "^5.4 || ^6.4",
        "symfony/twig-bundle": "^5.4 || ^6.4",
        "symfony/validator": "^5.4 || ^6.4",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "matthiasnoback/symfony-config-test": "^5.1",
        "matthiasnoback/symfony-dependency-injection-test": "^5.1",
        "phpunit/phpunit": "^9.6",
        "runroom-packages/testing": "^0.19",
        "sonata-project/block-bundle": "^4.13 || ^5.0",
        "sonata-project/doctrine-extensions": "^2.0",
        "sonata-project/doctrine-orm-admin-bundle": "^4.3",
        "sonata-project/form-extensions": "^1.18 || ^2.0",
        "sonata-project/twig-extensions": "^2.0",
        "symfony/browser-kit": "^5.4 || ^6.4",
        "symfony/mailer": "^5.4 || ^6.4",
        "symfony/mime": "^5.4 || ^6.4",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/yaml": "^5.4 || ^6.4",
        "symfonycasts/reset-password-bundle": "^1.9",
        "zenstruck/foundry": "^1.38.2 || ^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Runroom\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\UserBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.19-dev"
        }
    }
}
