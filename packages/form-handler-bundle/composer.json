{
    "name": "runroom-packages/form-handler-bundle",
    "type": "symfony-bundle",
    "description": "Alternative way to handle forms in Symfony",
    "keywords": [
        "form",
        "handler",
        "symfony"
    ],
    "homepage": "https://github.com/runroom/RunroomFormHandlerBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/config": "^4.4 || ^5.3",
        "symfony/dependency-injection": "^4.4 || ^5.3",
        "symfony/event-dispatcher": "^4.4 || ^5.3",
        "symfony/form": "^4.4 || ^5.3",
        "symfony/http-foundation": "^4.4.7 || ^5.3",
        "symfony/http-kernel": "^4.4.13 || ^5.3"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^9.5",
        "runroom-packages/render-event-bundle": "^0.14",
        "symfony/phpunit-bridge": "^5.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.13-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\FormHandlerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\FormHandlerBundle\\Tests\\": "tests/"
        }
    }
}
