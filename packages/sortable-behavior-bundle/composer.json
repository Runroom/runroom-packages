{
    "name": "runroom-packages/sortable-behavior-bundle",
    "type": "symfony-bundle",
    "description": "Provides a way to sort your admin listing",
    "keywords": [
        "sortable",
        "admin",
        "sonata"
    ],
    "homepage": "https://github.com/runroom/RunroomSortableBehaviorBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "doctrine/common": "^2.12 || ^3.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "symfony/config": "^4.4 || ^5.1",
        "symfony/dependency-injection": "^4.4 || ^5.1",
        "symfony/http-foundation": "^4.4.7 || ^5.1",
        "symfony/http-kernel": "^4.4.13 || ^5.1.5",
        "symfony/property-access": "^4.4 || ^5.1",
        "symfony/translation": "^4.4 || ^5.1",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "gedmo/doctrine-extensions": "^2.4 || ^3.0",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^9.3",
        "runroom-packages/testing": "^0.10",
        "sonata-project/admin-bundle": "^3.100 || ^4.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.34 || ^4.0",
        "symfony/phpunit-bridge": "^5.1"
    },
    "suggest": {
        "sonata-project/admin-bundle": "Can be easily integrated with Sonata Admin bundle",
        "stof/doctrine-extensions-bundle": "Manage doctrine extensions with Symfony"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\SortableBehaviorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\SortableBehaviorBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
