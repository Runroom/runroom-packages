{
    "name": "runroom-packages/seo-bundle",
    "description": "Improve SEO on your Symfony projects using SonataAdminBundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "seo",
        "symfony"
    ],
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "homepage": "https://github.com/runroom/RunroomSeoBundle",
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/orm": "^2.8",
        "doctrine/persistence": "^2.0 || ^3.0",
        "knplabs/doctrine-behaviors": "^2.0.6",
        "sonata-project/doctrine-extensions": "^1.13 || ^2.0",
        "sonata-project/media-bundle": "^3.34.2 || ^4.0",
        "symfony/config": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-foundation": "^4.4.7 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4.13 || ^5.4 || ^6.0",
        "symfony/property-access": "^4.4 || ^5.4 || ^6.0",
        "symfony/routing": "^4.4 || ^5.4 || ^6.0",
        "symfony/string": "^5.4 || ^6.0",
        "symfony/validator": "^4.4 || ^5.4 || ^6.0",
        "twig/string-extra": "^3.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "a2lix/auto-form-bundle": "^0.4",
        "a2lix/translation-form-bundle": "^3.0.4",
        "doctrine/annotations": "^1.13 || ^2.0",
        "knplabs/knp-menu-bundle": "^3.1",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^9.5",
        "runroom-packages/testing": "^0.17",
        "sonata-project/admin-bundle": "^3.103 || ^4.9",
        "sonata-project/doctrine-orm-admin-bundle": "^3.34 || ^4.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/security-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/twig-bundle": "^4.4 || ^5.4 || ^6.0",
        "zenstruck/foundry": "^1.13.2"
    },
    "conflict": {
        "a2lix/translation-form-bundle": "<3.0.4",
        "sonata-project/admin-bundle": "<3.103"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Runroom\\SeoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\SeoBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.17-dev"
        }
    }
}
