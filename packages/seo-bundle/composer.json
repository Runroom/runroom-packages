{
    "name": "runroom-packages/seo-bundle",
    "type": "symfony-bundle",
    "description": "Improve SEO on your Symfony projects using SonataAdminBundle",
    "keywords": [
        "seo",
        "symfony"
    ],
    "homepage": "https://github.com/runroom/RunroomSeoBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "a2lix/translation-form-bundle": "^2.4 || ^3.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/persistence": "^1.3",
        "knplabs/doctrine-behaviors": "^2.0",
        "runroom-packages/render-event-bundle": "^0.6.2",
        "sonata-project/admin-bundle": "^3.61",
        "sonata-project/media-bundle": "^3.24",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/http-foundation": "^4.4.7 || ^5.0.7",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/routing": "^4.4 || ^5.0",
        "symfony/validator": "^4.4 || ^5.0",
        "twig/string-extra": "^3.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "conflict": {
        "phpunit/phpunit": "9.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.7-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\SeoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\SeoBundle\\Tests\\": "tests/"
        }
    }
}
