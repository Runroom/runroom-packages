{
    "name": "runroom-packages/basic-page-bundle",
    "description": "Basic pages with only a wysiwyg and a title using Sonata",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "basic",
        "page",
        "sonata",
        "wysiwyg"
    ],
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "homepage": "https://github.com/runroom/RunroomBasicPageBundle",
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.9",
        "doctrine/persistence": "^2.0",
        "gedmo/doctrine-extensions": "^3.0",
        "knplabs/doctrine-behaviors": "^2.0.6",
        "psr/container": "^1.1 || ^2.0",
        "runroom-packages/seo-bundle": "^0.15",
        "symfony/asset": "^4.4 || ^5.4 || ^6.0",
        "symfony/config": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.4 || ^6.0",
        "symfony/form": "^4.4 || ^5.4 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-foundation": "^4.4.7 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4.13 || ^5.4 || ^6.0",
        "symfony/validator": "^4.4 || ^5.4 || ^6.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "a2lix/auto-form-bundle": "^0.3.1",
        "a2lix/translation-form-bundle": "^3.0.4",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "knplabs/knp-menu-bundle": "^3.1",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^9.5",
        "runroom-packages/testing": "^0.15",
        "sonata-project/admin-bundle": "^3.103 || ^4.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.34 || ^4.0",
        "sonata-project/media-bundle": "^3.34.2 || ^4.0@rc",
        "symfony/browser-kit": "^5.4 || ^6.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/routing": "^4.4 || ^5.4 || ^6.0",
        "symfony/security-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/twig-bundle": "^4.4 || ^5.4 || ^6.0",
        "zenstruck/foundry": "^1.13.2"
    },
    "conflict": {
        "a2lix/translation-form-bundle": "<3.0.4",
        "friendsofsymfony/ckeditor-bundle": "<2.2",
        "sonata-project/admin-bundle": "<3.103"
    },
    "autoload": {
        "psr-4": {
            "Runroom\\BasicPageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\BasicPageBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.14-dev"
        }
    }
}
