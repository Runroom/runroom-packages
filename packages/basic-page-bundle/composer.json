{
    "name": "runroom-packages/basic-page-bundle",
    "type": "symfony-bundle",
    "description": "Basic pages with only a wysiwyg and a title using Sonata",
    "keywords": [
        "basic",
        "page",
        "sonata",
        "wysiwyg"
    ],
    "homepage": "https://github.com/runroom/RunroomBasicPageBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "a2lix/translation-form-bundle": "^3.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "doctrine/persistence": "^1.3 || ^2.0",
        "friendsofsymfony/ckeditor-bundle": "^1.2 || ^2.2",
        "gedmo/doctrine-extensions": "^2.4 || ^3.0",
        "knplabs/doctrine-behaviors": "^2.0.6",
        "runroom-packages/render-event-bundle": "^0.10",
        "runroom-packages/seo-bundle": "^0.10",
        "sonata-project/admin-bundle": "^3.100 || ^4.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.34 || ^4.0",
        "symfony/config": "^4.4 || ^5.1",
        "symfony/dependency-injection": "^4.4 || ^5.1",
        "symfony/event-dispatcher": "^4.4 || ^5.1",
        "symfony/form": "^4.4 || ^5.1",
        "symfony/http-foundation": "^4.4.7 || ^5.1",
        "symfony/http-kernel": "^4.4.13 || ^5.1.5",
        "symfony/validator": "^4.4 || ^5.1"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^9.3",
        "runroom-packages/testing": "^0.10",
        "symfony/framework-bundle": "^4.4 || ^5.1",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/routing": "^4.4 || ^5.1",
        "zenstruck/foundry": "^1.10"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\BasicPageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\BasicPageBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
