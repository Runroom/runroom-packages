{
    "name": "runroom-packages/basic-page-bundle",
    "description": "Basic pages with only a wysiwyg and a title using Sonata",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "basic",
        "page",
        "sonata",
        "wysiwyg"
    ],
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "homepage": "https://github.com/runroom/RunroomBasicPageBundle",
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/orm": "^2.14",
        "doctrine/persistence": "^3.1",
        "gedmo/doctrine-extensions": "^3.11",
        "knplabs/doctrine-behaviors": "^2.6",
        "psr/container": "^1.1 || ^2.0",
        "runroom-packages/seo-bundle": "^0.18",
        "symfony/asset": "^5.4 || ^6.2",
        "symfony/config": "^5.4 || ^6.2",
        "symfony/dependency-injection": "^5.4 || ^6.2",
        "symfony/event-dispatcher": "^5.4 || ^6.2",
        "symfony/form": "^5.4 || ^6.2",
        "symfony/framework-bundle": "^5.4 || ^6.2",
        "symfony/http-foundation": "^5.4 || ^6.2",
        "symfony/http-kernel": "^5.4 || ^6.2",
        "symfony/validator": "^5.4 || ^6.2",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "a2lix/auto-form-bundle": "^0.4",
        "a2lix/translation-form-bundle": "^3.2",
        "dama/doctrine-test-bundle": "^7.2",
        "friendsofsymfony/ckeditor-bundle": "^2.3",
        "knplabs/knp-menu-bundle": "^3.1",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "phpunit/phpunit": "^9.6",
        "runroom-packages/testing": "^0.18",
        "sonata-project/admin-bundle": "^4.20",
        "sonata-project/doctrine-orm-admin-bundle": "^4.3",
        "sonata-project/media-bundle": "^4.5",
        "symfony/browser-kit": "^5.4 || ^6.2",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/routing": "^5.4 || ^6.2",
        "symfony/security-bundle": "^5.4 || ^6.2",
        "symfony/twig-bundle": "^5.4 || ^6.2",
        "zenstruck/foundry": "^1.34"
    },
    "conflict": {
        "a2lix/translation-form-bundle": "<3.0.4",
        "friendsofsymfony/ckeditor-bundle": "<2.2",
        "sonata-project/admin-bundle": "<4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Runroom\\BasicPageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\BasicPageBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.18-dev"
        }
    }
}
