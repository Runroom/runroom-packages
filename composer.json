{
    "name": "runroom-packages/runroom-packages",
    "description": "Runroom packages for Symfony development",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/common": "^3.1",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.9",
        "doctrine/persistence": "^2.0 || ^3.0",
        "gedmo/doctrine-extensions": "^3.0",
        "knplabs/doctrine-behaviors": "^2.0.6",
        "phpunit/phpunit": "^9.5",
        "psr/container": "^1.1 || ^2.0",
        "sonata-project/admin-bundle": "^3.103 || ^4.9",
        "sonata-project/doctrine-extensions": "^1.13",
        "sonata-project/media-bundle": "^3.34.2 || ^4.0",
        "symfony/asset": "^4.4 || ^5.4 || ^6.0",
        "symfony/config": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/doctrine-bridge": "^4.4 || ^5.4 || ^6.0",
        "symfony/error-handler": "^4.4 || ^5.4 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.4 || ^6.0",
        "symfony/event-dispatcher-contracts": "^1.1 || ^2.0 || ^3.0",
        "symfony/form": "^4.4 || ^5.4 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-foundation": "^4.4.7 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4.13 || ^5.4 || ^6.0",
        "symfony/options-resolver": "^4.4 || ^5.4 || ^6.0",
        "symfony/property-access": "^4.4 || ^5.4 || ^6.0",
        "symfony/routing": "^4.4 || ^5.4 || ^6.0",
        "symfony/security-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/security-core": "^4.4 || ^5.4 || ^6.0",
        "symfony/security-http": "^4.4 || ^5.4 || ^6.0",
        "symfony/string": "^5.4 || ^6.0",
        "symfony/translation": "^4.4 || ^5.4 || ^6.0",
        "symfony/twig-bundle": "^4.4 || ^5.4 || ^6.0",
        "symfony/validator": "^4.4 || ^5.4 || ^6.0",
        "symfony/yaml": "^4.4 || ^5.4 || ^6.0",
        "twig/string-extra": "^3.0",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "a2lix/auto-form-bundle": "^0.4",
        "a2lix/translation-form-bundle": "^3.0.4",
        "doctrine/data-fixtures": "^1.4",
        "doctrine/dbal": "^2.10 || ^3.0",
        "ergebnis/composer-normalize": "^2.2",
        "fakerphp/faker": "^1.13",
        "friendsofphp/php-cs-fixer": "^3.0",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "knplabs/knp-menu-bundle": "^3.1",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "nelmio/alice": "^3.7",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "psalm/plugin-phpunit": "^0.17",
        "psalm/plugin-symfony": "^3.0",
        "psr/event-dispatcher": "^1.0",
        "rector/rector": "^0.13",
        "sonata-project/block-bundle": "^3.23 || ^4.11",
        "sonata-project/doctrine-orm-admin-bundle": "^3.34 || ^4.0",
        "sonata-project/twig-extensions": "^1.9",
        "symfony/browser-kit": "^5.4 || ^6.0",
        "symfony/mailer": "^4.4 || ^5.4 || ^6.0",
        "symfony/mime": "^4.4 || ^5.4 || ^6.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfonycasts/reset-password-bundle": "^1.9",
        "symplify/monorepo-builder": "^11.0",
        "theofidry/alice-data-fixtures": "^1.3",
        "vimeo/psalm": "^4.0",
        "weirdan/doctrine-psalm-plugin": "^2.0",
        "zenstruck/foundry": "^1.13.2"
    },
    "replace": {
        "runroom-packages/basic-page-bundle": "0.15.7",
        "runroom-packages/ckeditor-sonata-media-bundle": "0.15.7",
        "runroom-packages/cookies-bundle": "0.15.7",
        "runroom-packages/form-handler-bundle": "0.15.7",
        "runroom-packages/redirection-bundle": "0.15.7",
        "runroom-packages/render-event-bundle": "0.15.7",
        "runroom-packages/seo-bundle": "0.15.7",
        "runroom-packages/sortable-behavior-bundle": "0.15.7",
        "runroom-packages/testing": "0.15.7",
        "runroom-packages/translation-bundle": "0.15.7",
        "runroom-packages/user-bundle": "0.15.7"
    },
    "conflict": {
        "a2lix/translation-form-bundle": "<3.0.4",
        "friendsofsymfony/ckeditor-bundle": "<2.2",
        "sonata-project/doctrine-orm-admin-bundle": "<3.34"
    },
    "autoload": {
        "psr-4": {
            "Runroom\\BasicPageBundle\\": "packages/basic-page-bundle/src/",
            "Runroom\\CkeditorSonataMediaBundle\\": "packages/ckeditor-sonata-media-bundle/src/",
            "Runroom\\CookiesBundle\\": "packages/cookies-bundle/src/",
            "Runroom\\FormHandlerBundle\\": "packages/form-handler-bundle/src/",
            "Runroom\\RedirectionBundle\\": "packages/redirection-bundle/src/",
            "Runroom\\RenderEventBundle\\": "packages/render-event-bundle/src/",
            "Runroom\\SeoBundle\\": "packages/seo-bundle/src/",
            "Runroom\\SortableBehaviorBundle\\": "packages/sortable-behavior-bundle/src/",
            "Runroom\\Testing\\": "packages/testing/src/",
            "Runroom\\TranslationBundle\\": "packages/translation-bundle/src/",
            "Runroom\\UserBundle\\": "packages/user-bundle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/",
            "Runroom\\BasicPageBundle\\Tests\\": "packages/basic-page-bundle/tests/",
            "Runroom\\CkeditorSonataMediaBundle\\Tests\\": "packages/ckeditor-sonata-media-bundle/tests/",
            "Runroom\\CookiesBundle\\Tests\\": "packages/cookies-bundle/tests/",
            "Runroom\\FormHandlerBundle\\Tests\\": "packages/form-handler-bundle/tests/",
            "Runroom\\RedirectionBundle\\Tests\\": "packages/redirection-bundle/tests/",
            "Runroom\\RenderEventBundle\\Tests\\": "packages/render-event-bundle/tests/",
            "Runroom\\SeoBundle\\Tests\\": "packages/seo-bundle/tests/",
            "Runroom\\SortableBehaviorBundle\\Tests\\": "packages/sortable-behavior-bundle/tests/",
            "Runroom\\Testing\\Tests\\": "packages/testing/tests/",
            "Runroom\\TranslationBundle\\Tests\\": "packages/translation-bundle/tests/",
            "Runroom\\UserBundle\\Tests\\": "packages/user-bundle/tests/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "scripts": {
        "normalize-run": [
            "composer normalize --no-check-lock --no-update-lock",
            "composer normalize --no-check-lock --no-update-lock packages/basic-page-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/ckeditor-sonata-media-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/cookies-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/form-handler-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/redirection-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/render-event-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/seo-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/sortable-behavior-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/testing/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/translation-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/user-bundle/composer.json"
        ],
        "php-cs-fixer": "@php php-cs-fixer fix --ansi --verbose",
        "phpstan": "@php phpstan analyse --no-progress",
        "psalm": "@php psalm --stats",
        "rector": "@php rector --no-progress-bar"
    }
}
