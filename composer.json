{
    "name": "runroom-packages/runroom-packages",
    "description": "Runroom packages for Symfony development",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.6 || ^4.0",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.19 || ^3.2",
        "doctrine/persistence": "^3.1 || ^4.0",
        "gedmo/doctrine-extensions": "^3.11",
        "phpunit/phpunit": "^10.5 || ^12.3",
        "psr/container": "^1.1 || ^2.0",
        "sonata-project/admin-bundle": "^4.25",
        "sonata-project/doctrine-extensions": "^2.0",
        "sonata-project/media-bundle": "^4.9",
        "symfony/asset": "^6.4 || ^7.1",
        "symfony/config": "^6.4 || ^7.1",
        "symfony/dependency-injection": "^6.4 || ^7.1",
        "symfony/doctrine-bridge": "^6.4 || ^7.1",
        "symfony/error-handler": "^6.4 || ^7.1",
        "symfony/event-dispatcher": "^6.4 || ^7.1",
        "symfony/event-dispatcher-contracts": "^2.1 || ^3.0",
        "symfony/form": "^6.4 || ^7.1",
        "symfony/framework-bundle": "^6.4 || ^7.1",
        "symfony/http-foundation": "^6.4 || ^7.1",
        "symfony/http-kernel": "^6.4 || ^7.1",
        "symfony/options-resolver": "^6.4 || ^7.1",
        "symfony/password-hasher": "^6.4 || ^7.1",
        "symfony/property-access": "^6.4 || ^7.1",
        "symfony/routing": "^6.4 || ^7.1",
        "symfony/security-bundle": "^6.4 || ^7.1",
        "symfony/security-core": "^6.4 || ^7.1",
        "symfony/security-http": "^6.4 || ^7.1",
        "symfony/string": "^6.4 || ^7.1",
        "symfony/translation": "^6.4 || ^7.1",
        "symfony/twig-bundle": "^6.4 || ^7.1",
        "symfony/validator": "^6.4 || ^7.1",
        "symfony/yaml": "^6.4 || ^7.1",
        "twig/string-extra": "^3.0",
        "twig/twig": "^3.14"
    },
    "require-dev": {
        "a2lix/auto-form-bundle": "^0.4",
        "a2lix/translation-form-bundle": "^3.2",
        "dama/doctrine-test-bundle": "^8.0",
        "ergebnis/composer-normalize": "^2.6",
        "friendsofphp/php-cs-fixer": "^3.14",
        "friendsofsymfony/ckeditor-bundle": "^2.3",
        "imagine/imagine": "^1.3",
        "knplabs/knp-menu-bundle": "^3.1",
        "matthiasnoback/symfony-config-test": "^6.1",
        "matthiasnoback/symfony-dependency-injection-test": "^6.1",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "psalm/plugin-phpunit": "^0.19",
        "psalm/plugin-symfony": "^5.0",
        "psr/event-dispatcher": "^1.0",
        "rector/rector": "^2.1",
        "sonata-project/block-bundle": "^5.0",
        "sonata-project/doctrine-orm-admin-bundle": "^4.13",
        "sonata-project/form-extensions": "^2.0",
        "sonata-project/twig-extensions": "^2.0",
        "symfony/browser-kit": "^6.4 || ^7.1",
        "symfony/mailer": "^6.4 || ^7.1",
        "symfony/mime": "^6.4 || ^7.1",
        "symfony/stimulus-bundle": "^2.30",
        "symfonycasts/reset-password-bundle": "^1.10",
        "symplify/monorepo-builder": "^11.0 <11.1",
        "vimeo/psalm": "^6.13",
        "weirdan/doctrine-psalm-plugin": "^2.6",
        "zenstruck/foundry": "^2.0"
    },
    "replace": {
        "runroom-packages/basic-page-bundle": "0.20.0",
        "runroom-packages/ckeditor-sonata-media-bundle": "0.20.0",
        "runroom-packages/cookies-bundle": "0.20.0",
        "runroom-packages/doctrine-translatable-bundle": "0.20.0",
        "runroom-packages/form-handler-bundle": "0.20.0",
        "runroom-packages/redirection-bundle": "0.20.0",
        "runroom-packages/render-event-bundle": "0.20.0",
        "runroom-packages/seo-bundle": "0.20.0",
        "runroom-packages/sortable-behavior-bundle": "0.20.0",
        "runroom-packages/testing": "0.20.0",
        "runroom-packages/translation-bundle": "0.20.0",
        "runroom-packages/user-bundle": "0.20.0"
    },
    "conflict": {
        "a2lix/translation-form-bundle": "<3.2",
        "friendsofsymfony/ckeditor-bundle": "<2.3",
        "sonata-project/admin-bundle": "<4.25",
        "sonata-project/doctrine-orm-admin-bundle": "<4.13",
        "zenstruck/foundry": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "Runroom\\BasicPageBundle\\": "packages/basic-page-bundle/src/",
            "Runroom\\CkeditorSonataMediaBundle\\": "packages/ckeditor-sonata-media-bundle/src/",
            "Runroom\\CookiesBundle\\": "packages/cookies-bundle/src/",
            "Runroom\\DoctrineTranslatableBundle\\": "packages/doctrine-translatable-bundle/src/",
            "Runroom\\FormHandlerBundle\\": "packages/form-handler-bundle/src/",
            "Runroom\\RedirectionBundle\\": "packages/redirection-bundle/src/",
            "Runroom\\RenderEventBundle\\": "packages/render-event-bundle/src/",
            "Runroom\\SeoBundle\\": "packages/seo-bundle/src/",
            "Runroom\\SortableBehaviorBundle\\": "packages/sortable-behavior-bundle/src/",
            "Runroom\\Testing\\": "packages/testing/src/",
            "Runroom\\TranslationBundle\\": "packages/translation-bundle/src/",
            "Runroom\\UserBundle\\": "packages/user-bundle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/",
            "Runroom\\BasicPageBundle\\Tests\\": "packages/basic-page-bundle/tests/",
            "Runroom\\CkeditorSonataMediaBundle\\Tests\\": "packages/ckeditor-sonata-media-bundle/tests/",
            "Runroom\\CookiesBundle\\Tests\\": "packages/cookies-bundle/tests/",
            "Runroom\\DoctrineTranslatableBundle\\Tests\\": "packages/doctrine-translatable-bundle/tests/",
            "Runroom\\FormHandlerBundle\\Tests\\": "packages/form-handler-bundle/tests/",
            "Runroom\\RedirectionBundle\\Tests\\": "packages/redirection-bundle/tests/",
            "Runroom\\RenderEventBundle\\Tests\\": "packages/render-event-bundle/tests/",
            "Runroom\\SeoBundle\\Tests\\": "packages/seo-bundle/tests/",
            "Runroom\\SortableBehaviorBundle\\Tests\\": "packages/sortable-behavior-bundle/tests/",
            "Runroom\\Testing\\Tests\\": "packages/testing/tests/",
            "Runroom\\TranslationBundle\\Tests\\": "packages/translation-bundle/tests/",
            "Runroom\\UserBundle\\Tests\\": "packages/user-bundle/tests/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "scripts": {
        "normalize-run": [
            "composer normalize --no-check-lock --no-update-lock",
            "composer normalize --no-check-lock --no-update-lock packages/basic-page-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/ckeditor-sonata-media-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/cookies-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/doctrine-translatable-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/form-handler-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/redirection-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/render-event-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/seo-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/sortable-behavior-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/testing/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/translation-bundle/composer.json",
            "composer normalize --no-check-lock --no-update-lock packages/user-bundle/composer.json"
        ],
        "php-cs-fixer": "@php php-cs-fixer fix --ansi --verbose",
        "phpstan": "@php phpstan analyse --no-progress",
        "psalm": "@php psalm --stats",
        "rector": "@php rector --no-progress-bar"
    }
}
